= render 'devise/shared/header2'
アカウントをお持ちでない方はこちら
= link_to root_path, class: "contents2__btn contents2__btn__mail" do
  新規会員登録
= link_to root_path, class: "contents2__btn contents2__btn__facebook" do
  = icon('fab', 'facebook-square fa-2x', class: "contents2__btn__icon2")
  Facebookでログイン
= link_to root_path, class: "contents2__btn contents2__btn__google" do
  = image_tag 'google_color.png', class: 'contents2__btn__icon3', size: '114x47'
  Googleでログイン
-# %h2 Log in
= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f|
  .field
    = f.label :email
    %br/
    = f.email_field :email, autofocus: true, autocomplete: "email"
  .field
    = f.label :password
    %br/
    = f.password_field :password, autocomplete: "current-password"
  
  .actions
    = f.submit "Log in"
= render "devise/shared/links"
= render 'devise/shared/footer2'




